<?xml version="1.0" encoding="UTF-8"?>
<web-app 
version="2.5"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
xmlns="http://java.sun.com/xml/ns/javaee" 
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" >

<display-name>Medicurator</display-name>

<servlet>
<servlet-name>Index</servlet-name>
<servlet-class>edu.emory.bmi.medicurator.servlet.Index</servlet-class>
</servlet>

<servlet>
<servlet-name>Signup</servlet-name>
<servlet-class>edu.emory.bmi.medicurator.servlet.Signup</servlet-class>
</servlet>

<servlet>
<servlet-name>CreateReplicaSet</servlet-name>
<servlet-class>edu.emory.bmi.medicurator.servlet.CreateReplicaSet</servlet-class>
</servlet>

<servlet>
<servlet-name>ShowReplicaSet</servlet-name>
<servlet-class>edu.emory.bmi.medicurator.servlet.ShowReplicaSet</servlet-class>
</servlet>

<servlet>
<servlet-name>SelectDataSet</servlet-name>
<servlet-class>edu.emory.bmi.medicurator.servlet.SelectDataSet</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>Index</servlet-name>
<url-pattern>/Index</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>Signup</servlet-name>
<url-pattern>/Signup</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>CreateReplicaSet</servlet-name>
<url-pattern>/CreateReplicaSet</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>ShowReplicaSet</servlet-name>
<url-pattern>/ShowReplicaSet</url-pattern>
</servlet-mapping>

<servlet-mapping>
<servlet-name>SelectDataSet</servlet-name>
<url-pattern>/SelectDataSet</url-pattern>
</servlet-mapping>

</web-app>

